<Window x:Class="disconnectedMOdel.Dataset"
        xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
        xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
        xmlns:d="http://schemas.microsoft.com/expression/blend/2008"
        xmlns:mc="http://schemas.openxmlformats.org/markup-compatibility/2006"
        xmlns:local="clr-namespace:disconnectedMOdel"
        mc:Ignorable="d"
        Title="Dataset" Height="450" Width="800">
    <Grid>
        <Button x:Name="btnLoad" Content="load data" HorizontalAlignment="Center" Margin="0,29,0,0" VerticalAlignment="Top" Height="40" Width="172" Click="btnLoad_Click"/>
        <DataGrid x:Name="grdCategories" d:ItemsSource="{d:SampleData ItemCount=5}" Margin="10,117,436,58"/>
        <DataGrid x:Name="grdProduct" d:ItemsSource="{d:SampleData ItemCount=5}" Margin="436,117,10,58"/>

    </Grid>
</Window>
